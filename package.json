{
  "name": "@codedungeon/gunner",
  "packageName": "gunner",
  "version": "0.51.0",
  "build": "261",
  "description": "Node CLI Framework",
  "info": "created in memory of my trusty sidekick \"Gunner\" who passed away in 2019.\n   his memory will last in my heart forever.",
  "author": "Mike Erickson <codedungeon@gmail.com> (https://github.com/mikeerickson)",
  "repository": "https://github.com/mikeerickson/gunner",
  "contributors": [
    {
      "name": "Mike Erickson",
      "email": "mike.erickson@codedungeon.io",
      "url": "https://github.com/mikeerickson"
    }
  ],
  "bin": {
    "gunner": "bin/gunner"
  },
  "main": "./src/gunner.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "bump": "./tasks/bumpBuild.js",
    "bump:dev": "./tasks/bumpBuild.js --verbose",
    "deploy": "node ./tasks/deploy.js && np --yolo",
    "lint": "node ./tasks/lint.js",
    "test": "./tasks/test.js $1",
    "test:all": "mocha ./test/*.test.js && node ./test/utils/testCleanup.js",
    "test:ci": "mocha --reporter dot && node ./test/utils/testCleanup.js",
    "test:spec": "mocha --reporter spec",
    "test:commit": "mocha ./test/*.test.js --reporter dot  && node ./test/utils/testCleanup.js",
    "test:coverage": "nyc mocha ./test/*.test.js && node ./test/utils/testCleanup.js",
    "test:watch": "mocha ./test/*.test.js --growl --watch --reporter progress",
    "todo": "node ./tasks/todo.js",
    "pre-commit": "npm run lint && npm run test:commit && npm run bump:dev && npm run todo"
  },
  "keywords": [
    "cli",
    "codedungeon",
    "framework",
    "javascript",
    "node",
    "starter",
    "skeleton"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "chai": "4.3.0",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "leasot": "11.5.0",
    "mocha": "6.0.2",
    "mock-fs": "4.13.0",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "sinon": "9.2.4"
  },
  "dependencies": {
    "@codedungeon/messenger": "0.11.0",
    "@codedungeon/utils": "0.0.12",
    "ansi-regex": "4.1.0",
    "apisauce": "2.0.1",
    "arg": "5.0.0",
    "begoo": "1.1.2",
    "chalk": "4.1.0",
    "clear-any-console": "1.16.2",
    "cli-table3": "0.6.0",
    "configstore": "5.0.1",
    "dashdash": "2.0.0",
    "dayjs": "1.10.4",
    "dot-prop": "6.0.1",
    "dumper.js": "1.3.1",
    "enquirer": "2.3.6",
    "execa": "5.0.0",
    "fs-chmod": "1.0.6",
    "fs-extra": "9.1.0",
    "fs-extra-promise": "1.0.1",
    "fs-jetpack": "4.1.0",
    "git-user-info": "1.0.1",
    "git-user-local": "1.0.0",
    "github-username": "6.0.0",
    "ip": "1.1.5",
    "jsonfile": "6.1.0",
    "min-dot": "0.1.1",
    "minimist": "1.2.5",
    "mustache": "4.1.0",
    "node-ip": "0.1.2",
    "npm-which": "3.0.1",
    "ora": "5.3.0",
    "please-upgrade-node": "3.2.0",
    "pluralize": "8.0.0",
    "semver": "7.3.5",
    "shelljs": "0.8.4",
    "strip-ansi": "5.0.0",
    "trash": "7.1.0",
    "update-notifier": "5.1.0",
    "valid-filename": "3.1.0",
    "voca": "1.4.0",
    "which": "2.0.2"
  }
}